{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}